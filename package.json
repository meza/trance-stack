  "browserslist": [
    "> 1%",
    "not dead",
    "not op_mini all"
  ],
  "workspaces": [
    "remix.init"
  ],
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ],
    "rules": {
      "header-max-length": [
        0,
        "always",
        100
      ]
    }
  },
  "jscpd": {
    "output": "./reports/jscpd",
    "pattern": "src/**/*.{js,jsx,ts,tsx}",
    "threshold": 0.1,
    "reporters": [
      "html",
      "console",
      "badge",
      "markdown"
    ],
    "ignore": [
      "**/__snapshots__/**",
      "**/*.test.(tsx|ts)"
    ],
    "absolute": true,
    "gitignore": true,
    "exitCode": 1
  },
