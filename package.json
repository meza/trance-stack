{
  "version": "0.0.0-dev",
  "private": true,
  "sideEffects": false,
  "scripts": {
    "build": "run-s build:remix",
    "build:remix": "remix build",
    "dev:remix": "cross-env NODE_ENV=development remix watch",
    "dev:arc": "cross-env NODE_ENV=development arc sandbox -p 3000 --verbose",
    "dev": "run-p dev:remix dev:arc",
    "ci": "run-s build && run-p lint report",
    "int": "playwright test",
    "lint": "run-p \"lint:*\"",
    "lint:css": "stylelint src/styles/**/*.css",
    "lint:eslint": "eslint --ext .ts,.tsx,.json src",
    "lint:typecheck": "tsc",
    "new": "plop",
    "new:component": "plop component",
    "new:util": "plop util",
    "new:hook": "plop hook",
    "prepare": "lefthook install",
    "release": "semantic-release",
    "report": "vitest --coverage",
    "start": "cross-env NODE_ENV=production arc sandbox -p 3000",
    "storybook": "run-s storybook:serve",
    "storybook:serve": "start-storybook -p 6005",
    "storybook:build": "build-storybook",
    "storybook:deploy": "gh-pages -d storybook-static -m 'chore(deploy): storybook to gh-pages'",
    "test": "run-p test:*",
    "test:unit": "vitest"
  },
  "dependencies": {
    "@aws-sdk/smithy-client": "^3.0.0",
    "@aws-sdk/types": "^3.0.0",
    "@remix-run/architect": "^1.13.0",
